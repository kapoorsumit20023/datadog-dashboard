    "widgets": [
        {
            "id": 0,
            "layout": {
                "x": 0,
                "y": 12,
                "width": 47,
                "height": 15
            },
            "definition": {
                "title": "Count of Hosts Reported via Datadog Agent",
                "title_size": "16",
                "title_align": "left",
                "show_legend": false,
                "legend_size": "0",
                "type": "timeseries",
                "requests": [
                    {
                        "response_format": "timeseries",
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "sum:system.cpu.user{*} by {host}"
                            }
                        ],
                        "formulas": [
                            {
                                "formula": "count_not_null(query1)"
                            }
                        ],
                        "style": {
                            "palette": "dog_classic",
                            "line_type": "solid",
                            "line_width": "normal"
                        },
                        "display_type": "line"
                    },
                    {
                        "response_format": "timeseries",
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "sum:system.cpu.user{*} by {host}"
                            }
                        ],
                        "formulas": [
                            {
                                "formula": "calendar_shift(count_not_null(query1), '-1w', 'UTC')"
                            }
                        ],
                        "style": {
                            "palette": "gray",
                            "line_type": "dotted",
                            "line_width": "normal"
                        },
                        "display_type": "line"
                    }
                ],
                "yaxis": {
                    "label": "",
                    "scale": "linear",
                    "include_zero": true,
                    "min": "auto",
                    "max": "auto"
                },
